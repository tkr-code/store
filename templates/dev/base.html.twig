                            {% endif %}
                            {% if site.twitter is defined %}
                            <a href="{{ site.twitter }}" class="social-icon social-twitter" title="Twitter"
                                target="_blank"><i class="icon-twitter"></i></a>
                            {% endif %}
                            {% if site.instagram is defined %}
                            <a href="{{ site.instagram }}" class="social-icon social-instagram" title="Pinterest"
                                target="_blank"><i class="icon-instagram"></i></a>
