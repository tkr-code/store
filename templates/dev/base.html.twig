<style>
    .logo img {
        width: 40px;
        border-radius: 15px;
    }

    .loader-app img {
        border-radius: 15px;
    }

    .logo span {
        margin-left: 3px;
        font-size: 18px;
        font-weight: bold;
    }
</style>

<body>
    {% if is_granted('ROLE_ADMIN') %}
