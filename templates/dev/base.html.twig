    <link rel="shortcut icon" href="{{ asset('img/favicon.ico') }}">
    <meta name="apple-mobile-web-app-title" content="{{ app_name }}">
    <meta name="application-name" content="{{ app_name }}">
    <meta name="msapplication-TileColor" content="#cc9966">
