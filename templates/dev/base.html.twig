                                                            <!-- End .menu-title -->
                                                            <ul>
                                                                <li>
                                                                    <a href="{{ path('cart_index') }}">{{'Cart'|trans}}</a>
                                                                </li>
                                                                {% if is_granted('ROLE_CLIENT') %}
