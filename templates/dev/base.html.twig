                                    <ul style="display: none;">
                                        <li>
                                            <a href="{{ path('client_index') }}">{{'Dashboard'|trans}}</a>
                                        </li>
                                        <li>
                                            <a href="{{ path('client_index',{tab:'orders'}) }}">Commande</a>
                                        </li>
                                        <li>
                                            <a href="{{ path('client_index',{tab:'buy'}) }}">Mes achats</a>
                                        </li>
                                        <li>
                                            <a href="{{ path('client_index',{tab:'details'}) }}">Details du
                                                compte</a>
                                        </li>
                                        <li>
                                            <a id="btn-logout" href="#">{{'Log out'|trans}}</a>
                                        </li>

                                    </ul>
                                </li>
                                {% endif %}
                            </ul><!-- End .menu -->
                        </nav><!-- End .main-nav -->

                        <button class="mobile-menu-toggler">
                            <span class="sr-only">Toggle mobile menu</span>
                            <i class="icon-bars"></i>
                        </button>
                    </div><!-- End .header-left -->

                    <div class="header-right">
                        <i class="la la-lightbulb-o"></i>
                        <p>Votre boutique mode Africaine</span></p>
                    </div>
                </div><!-- End .container -->
            </div><!-- End .header-bottom -->
